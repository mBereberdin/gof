{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/logs.txt"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "IsUseSwagger": true,
  "IsDevelopment": true,
  "ServiceName": "gof"
}
